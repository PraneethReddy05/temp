@prefix : <http://example.org/ontology#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<http://example.org/ontology> a owl:Ontology .

:Another_Paper a :Paper ;
    rdfs:label "A Different Paper" ;
    :hasAuthor :Other_Author .

:Paper_AI_2018 a :Paper ;
    rdfs:label "AI in Practice 2018" ;
    :hasAuthor :Andrew_Ng .

:Paper_DL_2020 a :Paper ;
    rdfs:label "Deep Learning Advancements 2020" ;
    :hasAuthor :Andrew_Ng .

:hasAuthor a owl:ObjectProperty ;
    rdfs:label "has author",
        "has author"@en ;
    rdfs:domain :Paper ;
    rdfs:range :Author,
        :Person .

:hasName a owl:DatatypeProperty ;
    rdfs:label "has name"@en ;
    rdfs:domain :Author ;
    rdfs:range xsd:string .

:hasTitle a owl:DatatypeProperty ;
    rdfs:label "has title"@en ;
    rdfs:domain :Paper ;
    rdfs:range xsd:string .

:hasTopic a owl:ObjectProperty ;
    rdfs:label "has topic"@en ;
    rdfs:domain :Paper ;
    rdfs:range :Topic .

:hasTopicName a owl:DatatypeProperty ;
    rdfs:label "has topic name"@en ;
    rdfs:domain :Topic ;
    rdfs:range xsd:string .

:Other_Author a :Author ;
    rdfs:label "Other Author" .

:Person a owl:Class ;
    rdfs:label "Person"@en ;
    rdfs:subClassOf owl:Thing .

:Paper a owl:Class ;
    rdfs:label "Paper",
        "Academic Paper"@en,
        "Research Paper"@en ;
    rdfs:subClassOf owl:Thing .

:Author a owl:Class ;
    rdfs:label "Author",
        "Author of a publication"@en ;
    rdfs:subClassOf owl:Thing .

